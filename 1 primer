<?php
$rows = 100;
$cols = 100;
$table = '<table border="2">';
for ($tr=1; $tr<=$rows; $tr++)
{ $table .= '<tr>';
for ($td=1; $td<=$cols; $td++)
{ if ($tr===1 or $td===1)
{ $table .= '<th text align="center">'. $tr*$td .'</th>'; }
else{ $table .= '<td>'.'</td>'; } }
$table .= '</tr>';}
$table .= '</table>';
echo $table;
?>
